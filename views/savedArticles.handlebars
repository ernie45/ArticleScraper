<nav class="navbar navbar-default lightBlue">
    <div class="container lightBlue">
        <div class="navbar-header lightBlue">
            <h1 class="navbar-brand" id="mongoTitle">Mongo Scraper</h1>
        </div>
        <div class="navbar-header darkerBlue">
            <a href="/">
                <h4 class="navbar-brand" id="home">Home</h4>
            </a>
        </div>
        <div class="navbar-header lightBlue">
            <a href="/savedArticles">
                <h4 class="navbar-brand" id="name">Saved Articles</h4>
            </a>
        </div>
    </div>
</nav>
<div class="container">
    <div id="middleText">
            <div id="giantText">MONGO SCRAPER</div>
            <div id="smallerText">NEW YORK TIMES EDITION</div>
    </div>
    <div class="container" id="savedArticleArea">
        <div id="savedStatus">
            <h1>No Saved Articles at this Moment in Time!</h1>
        </div>
        <div id="savedarticlesDisplay">
            {{#each articles}}
                <div class="saved" id={{this.id}}><a href={{this.hyperlink}}><span>{{this.header}}</span></a></div>
                <button type="button" class="btn btn-secondary">Secondary</button>
                <button type="button" class="btn btn-danger">Danger</button>
            {{/each}}
        </div>
    </div>
</div>